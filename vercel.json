{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://peronalknowledgebasechatbot-production.up.railway.app/api/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://peronalknowledgebasechatbot-production.up.railway.app"
  }
} 